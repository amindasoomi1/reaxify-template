{
  "name": "reaxify-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "prebuild": "npm audit fix && node scripts/generate-firebase-sw.cjs && node scripts/generate-robots.cjs",
    "build": "tsc && vite build",
    "generate-swagger-types": "npx swagger-typescript-api generate --path SWAGGER_URL -o ./src/types --name api.ts --no-client",
    "remove-optional-from-generated-swagger-types": "node scripts/remove-optional-from-swagger-types.cjs",
    "generate-types": "npm run generate-swagger-types && npm run remove-optional-from-generated-swagger-types"
  },
  "dependencies": {
    "@microsoft/signalr": "^10.0.0",
    "@tanstack/react-query": "^5.90.6",
    "axios": "^1.11.0",
    "castium": "^1.0.25",
    "firebase": "^12.6.0",
    "i18next": "^25.0.2",
    "iconsax-react": "^0.0.8",
    "lodash": "^4.17.21",
    "path": "^0.12.7",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-error-boundary": "^6.0.0",
    "react-form-rules": "^0.0.14",
    "react-hot-toast": "^2.5.2",
    "react-i18next": "^16.0.0",
    "react-router-dom": "^7.5.3",
    "reaxify": "^0.0.82",
    "tailwind-merge": "^3.2.0",
    "use-context-selector": "^2.0.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.16",
    "@tanstack/eslint-plugin-query": "^5.83.1",
    "@types/lodash": "^4.17.16",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.3",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vitejs/plugin-react-swc": "^4.1.0",
    "dotenv": "^17.2.3",
    "eslint": "^8.57.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.8.3",
    "vite": "^7.1.8",
    "vite-plugin-checker": "^0.11.0",
    "vite-plugin-pwa": "^1.1.0",
    "vite-plugin-svgr": "^4.5.0"
  }
}
